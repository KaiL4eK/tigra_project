<?xml version="1.0"?>

<!-- ros-kinetic-rosserial-msgs required!! -->

<launch>
    <group ns="car">
        <node pkg="rosserial_python" type="serial_node.py" name="car_serial" output="screen" respawn="true">
            <!-- <param name="port" value="/dev/serial/by-id/usb-1a86_USB2.0-Serial-if00-port0" />  -->
            <param name="port" value="/dev/ttyUSB0" /> 
            <param name="baud" value="115200" /> 
        </node> 
    </group>
</launch>
